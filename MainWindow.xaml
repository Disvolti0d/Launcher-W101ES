                        </Setter.Value>
                    </Setter>
                    <Style.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Background" Value="Black"/>
                            <Setter Property="Foreground" Value="White"/>
                        </Trigger>
                    </Style.Triggers>
                </Style>
            </Button.Style>
        </Button>
        <Image Source="/Wizard101-Logo.png" Margin="6,4,879,467" RenderTransformOrigin="0.43,0.427">
            <Image.Effect>
                <DropShadowEffect BlurRadius="5" ShadowDepth="10" Direction="279.7" RenderingBias="Quality"/>
            </Image.Effect>
        </Image>
        <Image Source="/Button_Spiral.png" Margin="5,0,0,43" Height="35" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="34"/>
        <Button Click="ImageButton_Click3" BorderThickness="0" Background="Transparent" Cursor="Hand" Margin="6,0,0,43" HorizontalAlignment="Left" Width="33" Height="34" VerticalAlignment="Bottom">
            <Button.Style>
                <Style TargetType="{x:Type Button}">
                    <Setter Property="Background" Value="Transparent"/>
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="{x:Type Button}">
                                <Border Background="{TemplateBinding Background}">
                                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
