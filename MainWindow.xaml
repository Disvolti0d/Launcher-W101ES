<Window x:Class="JuegoLauncher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JuegoLauncher"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        ContentRendered="Window_ContentRendered"
        Title="Wizard101 ESP (Launcher)" MinHeight="622" MinWidth="1106" MaxHeight="622" MaxWidth="1106">
    <Grid>
        <Image Stretch="UniformToFill" Source="/imagenes/image.jpg" HorizontalAlignment="Center" Margin="0,0,0,-3" Width="1106"/>
        <TextBlock Name="VersionText" VerticalAlignment="Bottom" HorizontalAlignment="Right" FontSize="18" Foreground="#FF716D6D" Margin="0, 0, 5, 0" FontFamily="Comic Sans MS"/>
        <Button Name="JugarBotón" Content="Comprobando actualizaciones" Click="JugarBotón_Click" Height="70" FontSize="28" Margin="0,450,0,0" VerticalAlignment="Top" HorizontalAlignment="Center" MinWidth="150" Width="436" BorderThickness="0,0,0,0" FontFamily="Comic Sans MS" FontWeight="Bold" UseLayoutRounding="False" Cursor="Hand">
            <Button.Effect>
                <DropShadowEffect x:Name="Sombreado" RenderingBias="Quality" ShadowDepth="50" BlurRadius="50" Opacity="1" Direction="315"/>
            </Button.Effect>
            <Button.OpacityMask>
                <ImageBrush ImageSource="/imagenes/setbackgroundplain.jpg"/>
            </Button.OpacityMask>
            <Button.BorderBrush>
                <ImageBrush/>
            </Button.BorderBrush>
            <Button.Background>
                <ImageBrush ImageSource="/imagenes/setbackgroundplain.jpg" Stretch="Fill" TileMode="Tile"/>
            </Button.Background>
            <Button.Style>
                <Style TargetType="{x:Type Button}">
                    <Setter Property="Background" Value="Transparent"/>
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="{x:Type Button}">
                                <Border Background="{TemplateBinding Background}">
                                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                </Border>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                    <Style.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Background" Value="Black"/>
                            <Setter Property="Foreground" Value="White"/>
                        </Trigger>
                    </Style.Triggers>
                </Style>
            </Button.Style>
        </Button>
        <Button Click="ImageButton_Click" BorderThickness="0" Background="Transparent" Cursor="Hand" RenderTransformOrigin="0.296,0.481" Margin="-2,0,0,-2" HorizontalAlignment="Left" Width="43" Height="44" VerticalAlignment="Bottom">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="-0.281"/>
                    <RotateTransform/>
                    <TranslateTransform X="-0.156"/>
                </TransformGroup>
            </Button.RenderTransform>
            <Image Source="/vecteezy_discord-logo-png-discord-icon-transparent-png_18930718.png" Width="58" Height="48" RenderTransformOrigin="-0.018,0.437" VerticalAlignment="Top" HorizontalAlignment="Left"/>
