            <Button.OpacityMask>
                <ImageBrush ImageSource="/imagenes/setbackgroundplain.jpg"/>
            </Button.OpacityMask>
            <Button.BorderBrush>
                <ImageBrush/>
            </Button.BorderBrush>
            <Button.Background>
                <ImageBrush ImageSource="/setbackground3.gif" Stretch="Fill" TileMode="Tile"/>
